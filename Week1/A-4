#include <iostream>
using namespace std;

int main() {
    int envelopes[][2] = {{5,4},{6,4},{6,7},{2,3}};
    int n = sizeof(envelopes) / sizeof(envelopes[0]);

    for (int i = 0; i < n - 1; i++) {
        for (int j = i + 1; j < n; j++) {
            if (envelopes[j][0] < envelopes[i][0] || 
               (envelopes[j][0] == envelopes[i][0] && envelopes[j][1] < envelopes[i][1])) {
                int w = envelopes[i][0], h = envelopes[i][1];
                envelopes[i][0] = envelopes[j][0];
                envelopes[i][1] = envelopes[j][1];
                envelopes[j][0] = w;
                envelopes[j][1] = h;
            }
        }
    }

    int dp[100000];
    for (int i = 0; i < n; i++) dp[i] = 1;

    int ans = 1;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < i; j++) {
            if (envelopes[j][0] < envelopes[i][0] && envelopes[j][1] < envelopes[i][1]) {
                if (dp[j] + 1 > dp[i]) dp[i] = dp[j] + 1;
            }
        }
        if (dp[i] > ans) ans = dp[i];
    }

    cout << ans;
}
