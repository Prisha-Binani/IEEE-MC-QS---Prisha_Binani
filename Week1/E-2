#include <iostream>
using namespace std;

int main() {
    int nums[] = {8, 1, 2, 2, 3};
    int size = sizeof(nums) / sizeof(nums[0]);
    int result[100];

    for (int i = 0; i < size; i++) {
        int count = 0;
        for (int j = 0; j < size; j++) {
            if (nums[j] < nums[i]) count++;
        }
        result[i] = count;
    }

    for (int i = 0; i < size; i++) {
        cout << result[i] << " ";
    }
}

